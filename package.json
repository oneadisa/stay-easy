{
  "name": "air-bnb",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start",
    "dev:emu": "cross-env USE_EMULATOR=1 expo start",
    "emu": "firebase emulators:start",
    "emu:export": "firebase emulators:start --export-on-exit=./emulator-data",
    "emu:import": "firebase emulators:start --import=./emulator-data",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "doctor": "expo-doctor",
    "precommit": "npm run typecheck && npm run lint"
  },
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/bottom-tabs": "^7.4.9",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "date-fns": "^4.1.0",
    "expo": "~54.0.13",
    "expo-auth-session": "~7.0.8",
    "expo-constants": "~18.0.9",
    "expo-file-system": "~19.0.17",
    "expo-image-picker": "~17.0.8",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.8",
    "firebase": "^12.4.0",
    "lucide-react-native": "^0.546.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "firebase-tools": "^14.20.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
